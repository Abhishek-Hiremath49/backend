    "class Student:\n",
    "    def __init__(self, name, age):\n",
    "        self.name = name   # Assign value to object attribute\n",
    "        self.age = age\n",
    "\n",
    "# Create objects\n",
    "s1 = Student(\"John\", 21)\n",
    "s2 = Student(\"Alice\", 22)\n",
    "\n",
    "print(s1.name, s1.age)  # John 21\n",
    "print(s2.name, s2.age)  # Alice 22\n"
   ]
  }
